<UserControl x:Class="Fixed_management.fixedadd"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="800">
    <Grid>
        <Label Content="资产条码" Height="28" HorizontalAlignment="Left" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="63,2,0,0" Name="barcode" VerticalAlignment="Top" Width="120" />
        <Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="189,2,0,0" Name="barcode_btn" VerticalAlignment="Top" Width="51" Click="barcode_btn_Click" />
        <Label Content="资产性质:" Height="28" HorizontalAlignment="Left" Margin="263,2,0,0" Name="label2" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Right" Margin="0,2,350,0" Name="textBox1" VerticalAlignment="Top" Width="120" />
        <Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="456,3,0,0" Name="nature_btn" VerticalAlignment="Top" Width="39" Click="nature_btn_Click" />
        <Label Content="资产类别:" Height="28" HorizontalAlignment="Left" Margin="514,2,0,0" Name="label3" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Right" Margin="0,6,102,0" Name="textBox2" VerticalAlignment="Top" Width="120" />
        <Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="704,5,0,0" Name="button2" VerticalAlignment="Top" Width="54" />
    </Grid>
</UserControl>
